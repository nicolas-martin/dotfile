{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    }
  },
  "python.jediEnabled": false,
  "python.pythonPath":"/usr/local/bin/python3",
  "python.upgradePythonLanguageServer":true,
  "suggest.noselect": false
}
