{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    }
  },
    "snippets.ultisnips.directories": [
    "gosnippets/UltiSnips"
  ],
  "python.jediEnabled": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.noselect": false
}
