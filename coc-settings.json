{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [ "go.mod", ".vim/", ".git/", ".hg/" ],
      "filetypes": [ "go" ]
    }
  },
    "snippets.ultisnips.directories": [
    "gosnippets/UltiSnips"
  ],
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "codeLens.enable": true,
  "python.jediEnabled": false
}
